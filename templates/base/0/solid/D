FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Patch1
    {

        type fixedValue;

        value uniform (0 0 0);
    }
    bottomEmptyFaces
    {

        type empty;
    }
    topEmptyFaces
    {

        type empty;
    }
}


